   <children>
      <VBox fx:id="outcomeVBox" alignment="CENTER" prefHeight="200.0" prefWidth="300.0" spacing="20.0">
         <children>
            <Label fx:id="outcomeLabel" text="&lt;Winner&gt; is the winner!" />
            <Button fx:id="outcomeButton" mnemonicParsing="false" text="Continue" onAction="#onOutcomeButtonClicked"/>
         </children>
      </VBox>
   </children>
</AnchorPane>
